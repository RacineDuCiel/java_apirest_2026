meta {
  name: 14 - Update My Account (USER)
  type: http
  seq: 14
}

put {
  url: http://localhost:8080/api/accounts/2
  body: json
  auth: bearer
}

auth:bearer {
  token: {{token_alice}}
}

body:json {
  {
    "username": "alice_paris",
    "email": "alice@paris.com",
    "address": {
      "street": "2 rue Nouvelle",
      "postalCode": "75003",
      "city": "Paris",
      "country": "France"
    }
  }
}

assert {
  res.status: eq 200
  res.body.username: eq alice_paris
  res.body.address.street: eq "2 rue Nouvelle"
}
