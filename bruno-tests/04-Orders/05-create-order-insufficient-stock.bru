meta {
  name: Create Order - Insufficient Stock
  type: http
  seq: 5
}

post {
  url: http://localhost:8080/api/orders
  body: json
  auth: bearer
}

auth:bearer {
  token: {{aliceToken}}
}

body:json {
  {
    "items": [
      {
        "productId": {{productId}},
        "quantity": 1000000
      }
    ]
  }
}

assert {
  res.status: eq 400
}
