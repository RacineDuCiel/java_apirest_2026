meta {
  name: 12 - Get My Account (USER)
  type: http
  seq: 12
}

get {
  url: http://localhost:8080/api/accounts/2
  body: none
  auth: bearer
}

auth:bearer {
  token: {{token_alice}}
}

assert {
  res.status: eq 200
  res.body.accountId: eq 2
  res.body.username: eq alice_paris
  res.body.email: eq alice@paris.com
  res.body.address: isDefined
}
