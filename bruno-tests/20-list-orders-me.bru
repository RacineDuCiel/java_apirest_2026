meta {
  name: 20 - List Orders (USER)
  type: http
  seq: 20
}

get {
  url: http://localhost:8080/api/orders/me?status=CREATED&size=5&sort=orderDate&direction=DESC
  body: none
  auth: bearer
}

auth:bearer {
  token: {{token_alice}}
}

assert {
  res.status: eq 200
  res.body.content: isArray
  res.body.content.length: gte 1
  res.body.content[0].status: eq CREATED
  res.body.content[0].orderId: eq {{order_alice_id}}
}
