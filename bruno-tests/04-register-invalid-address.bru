meta {
  name: 04 - Register INVALID Address (should fail)
  type: http
  seq: 4
}

post {
  url: http://localhost:8080/api/auth/register
  body: json
  auth: none
}

body:json {
  {
    "username": "invalid_user",
    "email": "invalid@test.com",
    "password": "Password123",
    "address": {
      "street": "Rue Inexistante 99999",
      "postalCode": "00000",
      "city": "VilleQuiNExistePas",
      "country": "France"
    }
  }
}

assert {
  res.status: eq 400
  res.body: contains L'adresse fournie n'est pas valide
}
