meta {
  name: 07 - Login ADMIN
  type: http
  seq: 7
}

post {
  url: http://localhost:8080/api/auth/login
  body: json
  auth: none
}

body:json {
  {
    "username": "admin",
    "password": "admin123"
  }
}

assert {
  res.status: eq 200
  res.body.token: isDefined
  res.body.username: eq admin
  res.body.email: eq admin@example.com
}

script:post-response {
  bru.setEnvVar("token_admin", res.body.token);
}
