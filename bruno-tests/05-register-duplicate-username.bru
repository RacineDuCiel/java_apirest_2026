meta {
  name: 05 - Register Duplicate Username (should fail)
  type: http
  seq: 5
}

post {
  url: http://localhost:8080/api/auth/register
  body: json
  auth: none
}

body:json {
  {
    "username": "alice_paris",
    "email": "alice_new@test.com",
    "password": "motdepasse123",
    "address": {
      "street": "10 rue de Rivoli",
      "postalCode": "75001",
      "city": "Paris",
      "country": "France"
    }
  }
}

assert {
  res.status: eq 400
  res.body: contains Ce nom d'utilisateur est déjà pris
}
